msgid "Unlock"
msgstr "Déverrouiller"

#: prefs.js:559 indicator.js:27
msgid "OTP code copied to clipboard."
msgstr "Code OTP copié dans le presse-papiers."

#: prefs.js:585
msgid "Edit this secret."
msgstr "Modifiez ce secret."

#: prefs.js:608
msgid "Editing TOTP secret"
msgstr "Modification du secret TOTP"

#: prefs.js:642
msgid "Export this secret to clipboard."
msgstr "Exportez ce secret dans le presse-papiers."

#: prefs.js:657
msgid "Copied secret URI to clipboard."
msgstr "URI secret copié dans le presse-papiers."

#: prefs.js:680
msgid "QR code"
msgstr "QR Code"

#: prefs.js:701 myAlertDialog.js:61
msgid "_Close"
msgstr "_Fermer"

