msgstr "Impossibile cercare il segreto."

#: secretUtils.js:180
msgid "Failed to retrieve secret."
msgstr "Impossibile recuperare il segreto."

#: secretUtils.js:220 secretUtils.js:252
msgid "Failed to set label."
msgstr "Impossibile impostare l'etichetta."

#: secretUtils.js:227
msgid "Failed to set attributes."
msgstr "Impossibile impostare gli attributi."

#: secretUtils.js:233
msgid "Failed to set secret."
msgstr "Impossibile impostare il segreto."

#: totp.js:34 totp.js:51
msgid "Invalid algorithm."
msgstr "Algoritmo non valido."

#: totp.js:118
msgid "Failed to parse URI."
msgstr "Impossibile analizzare l'URI."

#: totp.js:120
msgid "URI scheme should be \"otpauth://...\""
msgstr "Lo schema URI dovrebbe essere \"otpauth://...\""

#: totp.js:122
msgid "URI host should be \"totp\""
msgstr "L'host URI dovrebbe essere \"totp\""

#: totp.js:124
msgid "Unexpected port number in URI."
msgstr "Numero di porta imprevisto nell'URI."
