msgid "Invalid algorithm."
msgstr "Algoritmo non valido."

#: src/totp.js:118
msgid "Failed to parse URI."
msgstr "Impossibile analizzare l'URI."

#: src/totp.js:120
msgid "URI scheme should be \"otpauth://...\""
msgstr "Lo schema URI dovrebbe essere \"otpauth://...\""

