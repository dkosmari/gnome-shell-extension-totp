#: prefs.js:286
msgid "Issuer"
msgstr "Emisor"

#: prefs.js:288
msgid ""
"The name of the organization (Google, Facebook, etc) that issued the OTP."
msgstr ""
"El nombre de la organización (Google, Facebook, etc.) que emitió la OTP."

#: prefs.js:294
msgid "Name"
msgstr "Nombre"

#: prefs.js:301
msgid "Secret"
msgstr "Secreto"

#: prefs.js:303
